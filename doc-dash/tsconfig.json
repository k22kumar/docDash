{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/Components*": [
        "./components/*"
      ],
      "@/Constants/*": [
        "./core/constants/*"
      ],
      "@/Interfaces/*": [
        "./core/interfaces/*"
      ],
      "@/Utils/*": [
        "./core/utils/*"
      ],
      "@/Styles/*": [
        "./styles/*"
      ]

    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
